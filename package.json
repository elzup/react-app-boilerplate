{
	"name": "create-react-app",
	"version": "0.1.0",
	"dependencies": {
		"@babel/cli": "^7.0.0-beta.38",
		"@babel/preset-env": "^7.0.0-beta.38",
		"autoprefixer": "7.2.5",
		"babel-core": "6.26.0",
		"babel-eslint": "8.2.1",
		"babel-jest": "^22.1.0",
		"babel-loader": "7.1.2",
		"babel-plugin-lodash": "^3.3.2",
		"babel-preset-react-app": "^3.1.1",
		"babel-runtime": "6.26.0",
		"camelcase-keys-recursive": "^0.8.2",
		"case-sensitive-paths-webpack-plugin": "2.1.1",
		"chalk": "2.3.1",
		"coveralls": "^3.0.0",
		"css-loader": "0.28.9",
		"dotenv": "5.0.0",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"enzyme-to-json": "^3.3.1",
		"eslint": "^4.18.1",
		"eslint-config-react-app": "^2.1.0",
		"eslint-loader": "1.9.0",
		"eslint-plugin-flowtype": "^2.45.0",
		"eslint-plugin-import": "^2.6.0",
		"eslint-plugin-jsx-a11y": "^5.1.1",
		"eslint-plugin-react": "^7.7.0",
		"extract-text-webpack-plugin": "3.0.2",
		"file-loader": "1.1.8",
		"firebase": "^4.10.0",
		"flow-bin": "^0.66.0",
		"fs-extra": "5.0.0",
		"gh-pages": "^1.1.0",
		"html-webpack-plugin": "2.30.1",
		"husky": "^0.14.3",
		"jest": "^22.4.0",
		"lint-staged": "^6.1.1",
		"lodash": "^4.17.5",
		"material-ui": "^0.20.0",
		"moment": "^2.20.1",
		"normalizr": "^3.2.4",
		"object-assign": "4.1.1",
		"postcss-flexbugs-fixes": "3.3.0",
		"postcss-loader": "2.1.0",
		"prettier": "^1.10.2",
		"promise": "8.0.1",
		"query-string": "^5.1.0",
		"raf": "3.4.0",
		"react": "^16.2.0",
		"react-dev-utils": "^5.0.0",
		"react-dom": "^16.2.0",
		"react-redux": "^5.0.7",
		"react-router-dom": "^4.2.2",
		"react-test-renderer": "^16.2.0",
		"redux": "^3.7.2",
		"redux-persist": "^5.9.1",
		"redux-thunk": "^2.2.0",
		"s2s": "^0.18.1",
		"s2s-plugins-redux": "^0.4.5",
		"style-loader": "0.20.2",
		"styled-components": "^3.1.6",
		"superagent": "^3.8.2",
		"sw-precache-webpack-plugin": "0.11.4",
		"url-loader": "0.6.2",
		"webpack": "3.11.0",
		"webpack-dev-server": "2.11.1",
		"webpack-manifest-plugin": "1.3.2",
		"whatwg-fetch": "2.0.3"
	},
	"devDependencies": {},
	"homepage": "https://elzup.github.io/react-app-boilerplate",
	"jest": {
		"collectCoverageFrom": ["src/**/*.{js,jsx,mjs}"],
		"setupFiles": ["<rootDir>/config/polyfills.js"],
		"testMatch": [
			"<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
			"<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
		],
		"testEnvironment": "node",
		"testURL": "http://localhost",
		"transform": {
			"^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
			"^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
			"^(?!.*\\.(js|jsx|mjs|css|json)$)":
				"<rootDir>/config/jest/fileTransform.js"
		},
		"transformIgnorePatterns": ["[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"],
		"moduleNameMapper": {
			"^react-native$": "react-native-web"
		},
		"moduleFileExtensions": [
			"web.js",
			"mjs",
			"js",
			"json",
			"web.jsx",
			"jsx",
			"node"
		]
	},
	"lint-staged": {
		"src/**/*.js": ["prettier --config ./.prettierrc --write", "git add"]
	},
	"private": true,
	"scripts": {
		"build": "node scripts/build.js",
		"coveralls":
			"cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
		"deploy": "npm run build && gh-pages -d build",
		"flow": "flow",
		"gen:env":
			"cp templates/temp.env .env.development.local && cp templates/temp.env .env.production.local",
		"precommit": "lint-staged",
		"start": "node scripts/start.js",
		"test": "node scripts/test.js --env=jsdom --coverage"
	}
}
