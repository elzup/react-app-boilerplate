{
	"name": "create-react-app",
	"version": "0.1.0",
	"private": true,
	"homepage": "https://elzup.github.io/react-app-boilerplate",
	"scripts": {
		"start": "node scripts/start.js",
		"build": "node scripts/build.js",
		"test": "node scripts/test.js --env=jsdom --coverage",
		"deploy": "npm run build && gh-pages -d build",
		"coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
		"flow": "flow",
		"gen:env": "cp templates/temp.env .env.development.local && cp templates/temp.env .env.production.local",
		"precommit": "lint-staged"
	},
	"dependencies": {
		"camelcase-keys-recursive": "^0.8.2",
		"firebase": "^4.9.0",
		"lodash": "^4.17.4",
		"material-ui": "^0.20.0",
		"moment": "^2.20.1",
		"normalizr": "^3.2.4",
		"query-string": "^5.0.1",
		"react": "^16.2.0",
		"react-dom": "^16.2.0",
		"react-redux": "^5.0.6",
		"react-router-dom": "^4.2.2",
		"redux": "^3.7.2",
		"redux-persist": "^5.5.0",
		"redux-thunk": "^2.2.0",
		"style-loader": "0.20.1",
		"styled-components": "^2.2.3",
		"superagent": "^3.8.2",
		"sw-precache-webpack-plugin": "0.11.4",
		"url-loader": "0.6.2",
		"webpack": "3.10.0",
		"webpack-dev-server": "2.11.1",
		"webpack-manifest-plugin": "1.3.2",
		"whatwg-fetch": "2.0.3"
	},
	"devDependencies": {
		"@babel/cli": "^7.0.0-beta.38",
		"@babel/preset-env": "^7.0.0-beta.38",
		"autoprefixer": "7.2.5",
		"babel-core": "6.26.0",
		"babel-eslint": "8.2.1",
		"babel-jest": "^22.1.0",
		"babel-loader": "7.1.2",
		"babel-plugin-lodash": "^3.3.2",
		"babel-plugin-s2s-variable-initializing": "^0.1.6",
		"babel-preset-react-app": "^3.1.1",
		"babel-runtime": "6.26.0",
		"case-sensitive-paths-webpack-plugin": "2.1.1",
		"chalk": "2.3.0",
		"coveralls": "^3.0.0",
		"css-loader": "0.28.9",
		"dotenv": "4.0.0",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"enzyme-to-json": "^3.3.1",
		"eslint": "^4.16.0",
		"eslint-config-react-app": "^2.1.0",
		"eslint-loader": "1.9.0",
		"eslint-plugin-flowtype": "^2.42.0",
		"eslint-plugin-import": "^2.6.0",
		"eslint-plugin-jsx-a11y": "^5.1.1",
		"eslint-plugin-react": "^7.6.1",
		"extract-text-webpack-plugin": "3.0.2",
		"file-loader": "1.1.6",
		"flow-bin": "^0.64.0",
		"fs-extra": "5.0.0",
		"gh-pages": "^1.1.0",
		"html-webpack-plugin": "2.30.1",
		"husky": "^0.14.3",
		"jest": "^22.1.4",
		"lint-staged": "^6.1.0",
		"object-assign": "4.1.1",
		"postcss-flexbugs-fixes": "3.3.0",
		"postcss-loader": "2.0.10",
		"prettier": "^1.10.2",
		"promise": "8.0.1",
		"raf": "3.4.0",
		"react-dev-utils": "^5.0.0",
		"react-test-renderer": "^16.2.0",
		"s2s": "^0.17.0",
		"s2s-plugins-redux": "^0.4.3"
	},
	"lint-staged": {
		"src/**/*.js": [
			"prettier --config ./.prettierrc --write",
			"git add"
		]
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,jsx,mjs}"
		],
		"setupFiles": [
			"<rootDir>/config/polyfills.js"
		],
		"testMatch": [
			"<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
			"<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
		],
		"testEnvironment": "node",
		"testURL": "http://localhost",
		"transform": {
			"^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
			"^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
			"^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
		},
		"transformIgnorePatterns": [
			"[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
		],
		"moduleNameMapper": {
			"^react-native$": "react-native-web"
		},
		"moduleFileExtensions": [
			"web.js",
			"mjs",
			"js",
			"json",
			"web.jsx",
			"jsx",
			"node"
		]
	}
}
